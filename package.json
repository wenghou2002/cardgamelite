{
  "name": "card-game",
  "version": "1.0.0",
  "description": "Card Game with Vue frontend and Spring Boot backend",
  "scripts": {
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && mvn spring-boot:run",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\""
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "motion": "^12.5.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
